<div class="container mt-5">
  <%= form_with model: @habits, url: habits_new_path  do |f| %>
    <div class="form-group">
      <%= f.label :habits %>
      <%= f.text_field :title %>
    </div><br>


    <div>
      <%= f.label :entry_date, style: "display: block" %>
      <%= f.date_field :entry_date %>
    </div>

    <div>
      <%= f.submit %>
    </div>
  <% end%>

  <%= link_to 'Back', root_path %>
</div>